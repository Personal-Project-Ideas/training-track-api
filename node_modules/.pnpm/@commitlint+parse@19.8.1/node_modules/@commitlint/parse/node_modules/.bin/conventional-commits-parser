#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/conventional-commits-parser@5.0.0/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules:/Users/marceloprates/Documents/Projects/node_modules:/Users/marceloprates/Documents/node_modules:/Users/marceloprates/node_modules:/Users/node_modules:/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules/@commitlint/parse/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules/@commitlint/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/conventional-commits-parser@5.0.0/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules:/Users/marceloprates/Documents/Projects/node_modules:/Users/marceloprates/Documents/node_modules:/Users/marceloprates/node_modules:/Users/node_modules:/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules/@commitlint/parse/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules/@commitlint/node_modules:/Users/marceloprates/Documents/Projects/my-train-track-api/node_modules/.pnpm/@commitlint+parse@19.8.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../conventional-commits-parser@5.0.0/node_modules/conventional-commits-parser/cli.mjs" "$@"
fi
